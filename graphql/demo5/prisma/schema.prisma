generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Books {
  id   String    @id @default(uuid())
  title String @map("book_title")
  name String @map("author_name")

  @@map("books")
}

